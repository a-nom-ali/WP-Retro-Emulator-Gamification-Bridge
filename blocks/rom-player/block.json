{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wp-gamify/rom-player",
	"version": "0.1.2",
	"title": "ROM Player",
	"category": "widgets",
	"icon": "games",
	"description": "Display a single ROM without a selector dropdown",
	"supports": {
		"align": true,
		"html": false
	},
	"textdomain": "wp-gamify-bridge",
	"attributes": {
		"romId": {
			"type": "string",
			"default": ""
		},
		"touchToggle": {
			"type": "boolean",
			"default": true
		},
		"showMeta": {
			"type": "boolean",
			"default": true
		},
		"className": {
			"type": "string"
		}
	}
}
